<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    	<!-- M2M Transfers sequences -->
        <record id="sequence_m2m_transfers" model="ir.sequence">
            <field name="name">M2M Transfers sequence</field>
            <field name="code">m2m.transfers</field>
            <field name="prefix">MM/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="virtual_grown_plants_location" model="stock.location">
            <field name="name">Grown Plants</field>
           	<field name="location_id" ref="stock.stock_location_locations_virtual"/>
            <field name="usage">inventory</field>
            <field name="posz">1</field>
            <field name="company_id"></field>
        </record>
    </data>
</odoo>
