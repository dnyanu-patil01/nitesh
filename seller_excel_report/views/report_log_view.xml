<odoo>

    <record id="view_report_log_tree" model="ir.ui.view">
        <field name="name">report.log.view.tree</field>
        <field name="model">seller.report.log</field>
        <field name="arch" type="xml">
            <tree string="Report Log" create="0" delete="0" edit="0">                
                <field name="name" string="Name"/>
                <field name="report_type" />
                <field name="report_taken_at" />
                <field name="user_id"/>
                <field name="seller_id"  optional="show"/>
            </tree>
        </field>
    </record>

    <record id="view_report_log_form" model="ir.ui.view">
        <field name="name">report.log.view.form</field>
        <field name="model">seller.report.log</field>
        <field name="arch" type="xml">
            <form string="Sellers" create="0" delete="0" edit="0" duplicate="0">
                <group>
                    <group>
                        <field name="name" string="Name"/>
                        <field name="report_type" />
                        <field name="report_taken_at" />
                        <field name="user_id"/>
                        <field name="seller_id"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

    <record id="action_seller_report_log_form" model="ir.actions.act_window">
        <field name="name">Seller Report Log</field>
        <field name="res_model">seller.report.log</field>
        <field name="view_mode">tree</field>
        <field name="type">ir.actions.act_window</field>
    </record>

    <menuitem id="menu_report_log" parent="seller_management.seller_report_root_menu" action="action_seller_report_log_form" sequence="50" groups="seller_management.group_sellers_management_manager"/>
</odoo>