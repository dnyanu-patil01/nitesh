<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    
	<!-- Index Topbar -->
	<template id="index_topbar_partners" name="Topbar Partner">
	    <nav class="navbar navbar-light border-top shadow-sm d-print-none mt-3 mb-3">
	        <div class="container">
	            <div class="d-flex flex-column flex-sm-row justify-content-between w-100">
	                <span class="navbar-brand h4 my-0 mr-auto">Partners</span>
	                <ul class="o_wevent_index_topbar_filters nav"/>
	                <div class="d-flex align-items-center flex-wrap pl-sm-3 pr-0">
	                <a role="button" class="btn btn-primary" t-attf-href="/add_partner">Add Family Members</a>
	                </div>
	            </div>
	        </div>
	    </nav>
	</template>
    
	<template id="portal_partners" name="Partners">
        <t t-call="portal.portal_layout">
        <t t-call="kanha_census.index_topbar_partners"/>
			<t t-set="breadcrumbs_searchbar" t-value="True"/>
            <t t-if="not partners">
                <div class="alert alert-warning mt8" role="alert">
                    There are no partners.
                </div>
            </t>
            <t t-if="partners" t-call="portal.portal_table">
          		<thead>
		     		<tr class="active">
		            	<th>Name</th>
		            	<th>Email</th>
		            	<th>Mobile</th>
		            	<th>Date of Birth</th>
		          	</tr>
		        </thead>
                <tbody>
                    <tr t-foreach="partners" t-as="partner">
                        <td>
                            <a t-attf-href="/partner/#{partner.id}?{{ keep_query() }}"><span t-field="partner.name"/></a>
                        </td>
                        <td>
                       		<t t-esc="partner.email" />
                        </td>
                        <td>
                       		<t t-esc="partner.mobile" />
                        </td>
                        <td>
                       		<t t-esc="partner.date_of_birth" />
                        </td>
                    </tr>
                </tbody>
            </t>
        </t>
    </template>

	<template id="portal_partner" name="Partner">
        <t t-call="portal.portal_layout">
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                    <h5 class="mb-0">
                        <span t-field="partner.name" class="text-truncate"/>
                        <span class="float-right">
                            <a role="button" class="btn btn-sm btn-primary">Edit</a>
                        </span>
                    </h5>
                </t>
                <t t-set="card_body">
                    <div class="row">
                        <div t-if="partner.id" class="col-12 col-md-6 mb-2 mb-md-0">
                            <h6>Customer</h6>
                            <div class="row">
                                <div class="col flex-grow-0 pr-3">
                                    <img t-if="partner.image_1024" class="rounded-circle mt-1 o_portal_contact_img" t-att-src="image_data_uri(partner.image_1024)" alt="Contact"/>
                                    <img t-else="" class="rounded-circle mt-1 o_portal_contact_img" src="/web/static/src/img/user_menu_avatar.png" alt="Contact"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </t>
    </template>
    
