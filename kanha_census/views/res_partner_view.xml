    </tree>
            <form string="Family Member">
                <group col="2">
                    <field name="name" required="1"/>
                    <field name="relation" required="1"/>
                    <field name="blood_group"/>
                    <field name="mobile" attrs="{'invisible':[('parent.citizenship','=','Indian')]}"/>
                    <field name="emergency_contact" attrs="{'invisible':[('parent.citizenship','=','Indian')]}"/>
                    <field name="govt_id_proof" filename="govt_id_proof_filename" widget="binary" invisible="1"/>
                    <field name="govt_id_proof_filename" invisible="1"/>
                    <field name="passport_photo" filename="passport_photo_filename" widget="binary" attrs="{'invisible':[('parent.citizenship','=','Indian')]}"/>
                    <field name="passport_photo_filename" invisible="1"/>
                    <field name="address_proof" filename="address_proof_filename" widget="binary" invisible="1"/>
                    <field name="address_proof_filename" invisible="1"/>
