<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	<record model="ir.ui.view" id="kanha_inherit_view_partner_form">
		<field name="name">kanha.inherit.view.partner.form</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="base.view_partner_form" />
		<field name="mode">primary</field>
		<field name="arch" type="xml">
			<xpath expr="//form" position="replace">
				<form string="Kanha Census">
					<header>
						<button name="button_approve" string="Approve" type="object" attrs="{'invisible':[('application_status','!=','to_approve')]}" class="btn-success"/>
						<button name="button_reject" string="Reject" type="object" attrs="{'invisible':[('application_status','!=','to_approve')]}" class="btn-danger"/>
						<button name="button_print_card" 
        string="Card Printed" 
        type="object" 
        attrs="{'invisible':[('application_status','not in',['Approved but Card Not Printed','Card Printed'])]}" 
        class="btn-primary"/>
						<button name="button_approval_for_edit" string="Approval for Edit" type="object" attrs="{'invisible':[('application_status','not in',['Approved but Card Not Printed','Card Printed','rejected'])]}" class="btn-warning"/>
						<field name="application_status" widget="statusbar" />
						<field name="is_geoadmin" invisible="1"/>
					</header>
					<sheet>
						<widget name="web_ribbon" title="Draft" bg_color="bg-secondary" attrs="{'invisible': [('application_status', '!=', 'draft')]}"/>
						<widget name="web_ribbon" title="To Approve" bg_color="bg-warning" attrs="{'invisible': [('application_status', '!=', 'to_approve')]}"/>
						<widget name="web_ribbon" title="Approved" bg_color="bg-success" attrs="{'invisible': [('application_status', '!=', 'Approved but Card Not Printed')]}"/>
						<widget name="web_ribbon" title="Card Printed" bg_color="bg-info" attrs="{'invisible': [('application_status', '!=', 'Card Printed')]}"/>
						<widget name="web_ribbon" title="Rejected" bg_color="bg-danger" attrs="{'invisible': [('application_status', '!=', 'rejected')]}"/>
						<widget name="web_ribbon" title="Approved for Edit" bg_color="bg-primary" attrs="{'invisible': [('application_status', '!=', 'approved_for_edit')]}"/>
						<field name="image_1920" widget="image" class="oe_avatar" options="{'preview_image': 'image_128'}"/>
						<group>
							<group>
								<field name="name" required="1" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="email" required="1" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="full_name_passport" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="gender" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="any_gov_id_proof" invisible="1" string="Any Govt. ID Proof: (eg. Masked Aadhar Card, Voter ID Card or Driving License)" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}" help="Masked Aadhar Card, Voter ID Card or Driving License"/>
								<field name="is_preceptor" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="abhyasi_id" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="srcm_id" invisible="1" attrs="{'invisible':[('citizenship','=','Indian')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								
								<field name="preceptor_incharge_name" attrs="{'invisible':[('citizenship','=','Indian')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="residence_type" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="id" string="Resident ID number" readonly="1"/>
								<field name="state" invisible="1"/>
								<field name="blood_group" invisible="1" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
							</group>
							<group>
								<field name="surname" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="mobile" required="1" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="year_of_birth" readonly="1"/>
								<field name="citizenship" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="passport_number" widget="Encrypted" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								
								<field name="birth_country_name" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="emergency_contact" invisible="1" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="property_owner_name" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="property_owner_email" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="property_owner_phone" attrs="{'invisible':[('citizenship','!=','Overseas')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="members_count" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="has_voter_id_in_kanha" attrs="{'invisible':[('citizenship','!=','Indian')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="voter_id_number_optional_id" attrs="{'invisible':['|',('citizenship','!=','Indian'),('has_voter_id_in_kanha','!=','Yes')], 'required':[('citizenship','=','Indian'),('has_voter_id_in_kanha','=','Yes')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="wants_to_apply_voter_id" attrs="{'invisible':['|',('citizenship','!=','Indian'),('has_voter_id_in_kanha','!=','No')], 'required':[('citizenship','=','Indian'),('has_voter_id_in_kanha','=','No')], 'readonly':[('application_status','not in',['draft','approved_for_edit'])]}"/>
								<field name="do_you_need_voter_id_in_kanha" invisible="1"/>
							</group>
						</group>
						<notebook>
							<page string="Family Details" name="family_details">
    <group>
        <field name="family_member_ids" widget="one2many" nolabel="1" attrs="{'readonly':[('application_status','not in',['draft','approved_for_edit'])]}">
    <tree string="Family Members" editable="bottom" limit="8" create="1" edit="1" delete="1">
        <field name="sequence" invisible="1"/>
        <field name="name" required="1" placeholder="Enter Name"/>
        <field name="relation" required="1"/>
        <field name="blood_group" required="1"/>
        <field name="mobile" required="1" attrs="{'column_invisible':[('parent.citizenship','=','Indian')]}"/>
        <field name="emergency_contact" required="1" attrs="{'column_invisible':[('parent.citizenship','=','Indian')]}"/>
        <field name="govt_id_proof" filename="govt_id_proof_filename" widget="binary" invisible="1"/>
        <field name="govt_id_proof_filename" invisible="1"/>
        <field name="passport_photo" filename="passport_photo_filename" widget="binary" required="1" attrs="{'column_invisible':[('parent.citizenship','=','Indian')]}"/>
        <field name="passport_photo_filename" invisible="1"/>
        <field name="address_proof" filename="address_proof_filename" widget="binary" invisible="1"/>
