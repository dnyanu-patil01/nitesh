<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		
		<!-- Residents Documents Downloads History Tree -->
	    <record id="residents_documents_downloads_history_view_tree" model="ir.ui.view">
	        <field name="name">residents.documents.downloads.history.tree</field>
	        <field name="model">residents.documents.downloads.history</field>
	        <field name="arch" type="xml">
	            <tree string="Documents Downloads History" create="false" edit="false">
	          		<field name="downloaded_by"/>
	          		<field name="downloaded_datetime"/>
	            </tree>
	        </field>
	    </record>
	
		<!-- Residents Documents Downloads History Form -->
	    <record id="residents_documents_downloads_history_view_form" model="ir.ui.view">
	        <field name="name">residents.documents.downloads.history.form</field>
	        <field name="model">residents.documents.downloads.history</field>
	        <field name="arch" type="xml">
	            <form string="Documents Downloads History" create="false" edit="false">
	                <sheet>
                        <group name="first" col="2">
                     		<field name="downloaded_by"/>
	          				<field name="downloaded_datetime"/>
                        </group>
                        <notebook>
                        	<page string="Downloads History" name="residents_documents_downloads_history" >
                    			<field name="partner_ids" colspan="4" nolabel="1">
	                            	<tree string="Downloads History">
	                               		<field name="name"/>
	                            	</tree>
	                            	<form string="Downloads History">
		                                <group col="4">
		                                    <field name="name" />
		                                </group>
	                            	</form>
                        		</field>
                    		</page>
                    	</notebook>
                    </sheet>
	            </form>
	        </field>
	    </record>
	
		<!-- Residence Documents Downloads History Action -->
	    <record id="residents_documents_downloads_history_action" model="ir.actions.act_window">
	        <field name="name">Documents Downloads History</field>
	        <field name="type">ir.actions.act_window</field>
	        <field name="res_model">residents.documents.downloads.history</field>
	        <field name="view_mode">tree,form</field>
	    </record>
	           
		<!-- Residents Documents Downloads History menu -->
		<menuitem
		    id="residents_documents_downloads_history_menu"
		    name="Documents Downloads History"
		    parent="kanha_census.menu_kanha_census_root"
		    action="residents_documents_downloads_history_action"
			sequence="40"
		    />

	</data>
</odoo>
