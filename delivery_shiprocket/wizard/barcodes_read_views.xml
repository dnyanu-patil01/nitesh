<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_barcodes_read_form" model="ir.ui.view">
        <field name="name">barcodes.read.form</field>
        <field name="model">wiz.barcodes.read</field>
        <field name="arch" type="xml">
            <form string="Scan Barcodes">
                <sheet>
                    <group>
                        <field name="_barcode_scanned" widget="barcode_handler" />
                        <field name="bulk_process_id" readonly="1"/>
                    </group>
                    <field name="stock_picking_ids" readonly="1" force_save="1">
                        <tree>
                            <field name="picking_id" />
                            <field name="shiprocket_awb_code" />
                            <field name="courier_id" />
                            <!-- <field name="is_serviceable" string="Serviceable" /> -->
                            <field name="shiprocket_order_status_id" />
                        </tree>
                    </field>
                </sheet>
                <footer>
                    <button name="generate_manifest_bulk" confirm="This action will be done by backgroud.We will notify you soon." type="object" string="Generate Manifest" icon="fa-cogs" class="oe_highlight" />
                    <button string="Cancel" class="btn btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <record id="action_barcodes_read" model="ir.actions.act_window">
        <field name="res_model">wiz.barcodes.read</field>
        <field name="name">Barcodes Read</field>
        <field name="view_mode">form</field>
        <field name="context">{}</field>
        <field name="view_id" ref="view_barcodes_read_form" />
        <field name="target">current</field>
    </record>
</odoo>