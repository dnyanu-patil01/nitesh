<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="view_delivery_carrier_form_inh" model="ir.ui.view">
            <field name="name">delivery.carrier.form.add.code</field>
            <field name="model">delivery.carrier</field>
            <field name="inherit_id"
                   ref="shopify_ept.view_delivery_carrier_form_ept"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="integration_level" position="after">
                    <field name="shopify_source" attrs="{'invisible':[('delivery_type','!=','shiprocket')]}"/>
                    <field name="shopify_code" attrs="{'invisible':[('delivery_type','!=','shiprocket')]}"/>
                </field>
            </field>
        </record>
    </data>
</odoo>
