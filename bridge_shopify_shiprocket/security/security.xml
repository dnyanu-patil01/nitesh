<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- Seller Pickup Location Rules -->
        <record id="seller_shiprocket_pickup_location_rule_user" model="ir.rule">
            <field name="name">Seller Shiprocket Pickup Location</field>
            <field ref="delivery_shiprocket.model_shiprocket_pickup_location" name="model_id" />
            <field name="domain_force">[('seller_id','=',user.seller_id.id)]</field>
            <field name="groups" eval="[(4,ref('seller_management.group_sellers_management_user'))]" />
        </record>
        <record id="seller_shiprocket_pickup_location_rule_manager" model="ir.rule">
            <field name="name">Seller Manager Shiprocket Pickup Location</field>
            <field ref="delivery_shiprocket.model_shiprocket_pickup_location" name="model_id" />
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups" eval="[(4,ref('seller_management.group_sellers_management_manager'))]" />
        </record>
    </data>
</odoo>